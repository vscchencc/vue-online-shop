{
  "name": "vue-online-shop",
  "version": "1.0.0",
  "description": "This a sample online shop",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack/webpack.config.js --open --hot --mode development",
    "build": "webpack --config ./webpack/webpack.prod.config.js --progress --hide-modules --mode production",
    "analyz": "cross-env NODE_ENV=production npm run build",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .vue src/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/",
    "commit": "git-cz",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cc-ch/vue-online-shop.git"
  },
  "keywords": [
    "vue",
    "online-shop"
  ],
  "author": "cc~cc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cc-ch/vue-online-shop/issues"
  },
  "homepage": "https://github.com/cc-ch/vue-online-shop#readme",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.5.3",
    "eslint-friendly-formatter": "^4.0.1",
    "file-loader": "^6.0.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.5",
    "vue-loader": "^15.9.2",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.3.0",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^1.0.0",
    "fibers": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "js-cookie": "^2.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mockjs": "^1.1.0",
    "sass": "^1.3.0",
    "standard-version": "^8.0.1",
    "vue-eslint-parser": "^7.0.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
